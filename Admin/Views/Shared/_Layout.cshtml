<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Brew A Gift | Administrator</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
    @Styles.Render("~/Content/css")

</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href='@Url.Action("DashBoard", "Home")' class="site_title"><i class="fa fa-gift"></i> <span>Brew A Gift</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu prile quick info -->
                    <div class="profile">
                        <div class="profile_pic">
                            <img src='@Session["AdminPic"]' alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Welcome</span>
                            <h2>@Session["AdminName"]</h2>
                        </div>
                    </div>
                    <!-- /menu prile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <br><br><br><br>
                            <ul class="nav side-menu">
                                <li><a href='@Url.Action("DashBoard", "Home")'><i class="fa fa-home"></i>Dashboard</a></li>
                                <li><a href='@Url.Action("Index", "Members")'><i class="fa fa-users"></i>Members Details</a></li>
                                <li><a href='@Url.Action("Index", "Events")'> <i class="fa fa-tasks"></i>Events Details</a></li>
                                <li><a href='@Url.Action("Index", "GiftItems")'><i class="fa fa-gift"></i>Gift Items Details</a></li>
                                <li><a href='@Url.Action("Index", "Payments")'><i class="fa fa-exchange"></i>Transactions Details</a></li>
                                <li><a href='@Url.Action("Index", "Orders")'><i class="fa fa-first-order"></i>Orders Details</a></li>
                                <li><a href='@Url.Action("Index", "Coupon")'><i class="fa fa-shopping-cart"></i>Coupon Details</a></li>
                                <li><a href='@Url.Action("Index", "Tickets")'><i class="fa fa-ticket"></i>Tickets Details</a></li>
                                <li><a href='@Url.Action("Index", "Vendors")'><i class="fa fa-black-tie"></i>Vendors Details</a></li>
                                <li><a href='@Url.Action("Index", "SubAdmin")'><i class="fa fa-user-plus"></i>Sub Administrators</a></li>
                            </ul>
                        </div>
                    </div>
                    <!-- /sidebar menu -->
                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav class="" role="navigation">
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <img src='@Session["AdminPic"]' alt="">@Session["AdminName"]
                                    <span class="fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu animated fadeInDown pull-right">
                                    <li><a href='@Url.Action("Logout", "Home")'><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            @RenderBody()
        </div>
    </div>
    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>
</html>
